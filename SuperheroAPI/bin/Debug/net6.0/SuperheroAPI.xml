<?xml version="1.0"?>
<doc>
    <assembly>
        <name>SuperheroAPI</name>
    </assembly>
    <members>
        <member name="M:SuperheroAPI.Controllers.SuperheroController.GetAll">
            <summary>
            Gets all heroes
            </summary>
            <returns>returns all heroes</returns>
        </member>
        <member name="M:SuperheroAPI.Controllers.SuperheroController.InsertSuperhero(DomainSuperhero.Models.SuperheroModel)">
            <summary>
            Creates an Superhero.
            </summary>
            <remarks>
            Sample request:
            
                POST api/Employee
                {        
                  "superheroID": 0,
                  "name": "Andrew",
                  "superheroName": "Amazing-SpiderMan",
                  "city": "Queens"
                }
            </remarks>
            <param name="hero"></param>      
        </member>
        <member name="M:SuperheroAPI.Controllers.SuperheroController.UpdateSuperhero(DomainSuperhero.Models.SuperheroModel)">
            <summary>
            Updates the specified hero chosen by you
            </summary>
            <param name="hero"></param>
            <returns>return the hero you have updated</returns>
        </member>
        <member name="M:SuperheroAPI.Controllers.SuperheroController.DeleteSuperhero(System.Int32)">
            <summary>
            deletes the hero
            </summary>
            <param name="id"></param>
            <returns>Hero is deleted</returns>
        </member>
    </members>
</doc>
